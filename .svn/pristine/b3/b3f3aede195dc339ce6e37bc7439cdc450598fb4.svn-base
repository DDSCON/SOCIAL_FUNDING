.b-pulldown {
	background-color: #fff;
	border: 1px solid #dfdfdf;
	border-radius: 6px;
	box-shadow: 0 1px 2px -1px rgba(0, 0, 0, 0.2);
	margin-top: 4px;
	position: fixed;
	padding: 6.5px 0;
	right: 1em;
	visibility: visible;
	z-index: 99;
	-webkit-transition: margin 0.18s ease-out, opacity 0.18s ease-out, width
		0.18s ease-out, height 0.18s ease-out, right 0.18s ease-out, top 0.18s
		ease-out;
	-moz-transition: margin 0.18s ease-out, opacity 0.18s ease-out, width
		0.18s ease-out, height 0.18s ease-out, right 0.18s ease-out, top 0.18s
		ease-out;
	-ms-transition: margin 0.18s ease-out, opacity 0.18s ease-out, width
		0.18s ease-out, height 0.18s ease-out, right 0.18s ease-out, top 0.18s
		ease-out;
	-o-transition: margin 0.18s ease-out, opacity 0.18s ease-out, width
		0.18s ease-out, height 0.18s ease-out, right 0.18s ease-out, top 0.18s
		ease-out;
	transition: margin 0.18s ease-out, opacity 0.18s ease-out, width 0.18s
		ease-out, height 0.18s ease-out, right 0.18s ease-out, top 0.18s
		ease-out
}

.b-pulldown:before {
	width: 10px;
	height: 10px;
	display: block;
	content: "";
	position: absolute;
	background-color: #fff;
	border: 1px solid #ddd;
	border-color: #ddd #ddd transparent transparent;
	top: -5px;
	right: 1.7em;
	z-index: -1;
	opacity: 1;
	transform: rotate(-45deg);
	-moz-transform: rotate(-45deg);
	-webkit-transform: rotate(-45deg);
	-o-transform: rotate(-45deg);
	-ms-transform: rotate(-45deg)
