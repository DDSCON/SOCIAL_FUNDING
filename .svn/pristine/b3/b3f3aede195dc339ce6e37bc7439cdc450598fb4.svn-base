.i-icon-search {
	display: none
}

.b-icon-search__inner { /* 검색 아이콘 돋보기 색깔  */
	border: 2px solid #06bfbf;
	cursor: pointer;
	display: block;
	height: 16px;
	position: relative;
	width: 16px;
	border-radius: 24px
}

.b-icon-search__inner:before { /* 검색 아이콘 돋보기 색깔  */
	content: "";
	position: absolute;
	display: block;
	width: 10px;
	height: 10px;
	top: 2px;
	left: 2px;
	border: 1px solid #06bfbf;
	border-color: transparent #06bfbf transparent transparent;
	border-radius: 30px;
	-webkit-border-radius: 30px;
	-moz-border-radius: 30px;
	-webkit-transition: all 0.2s ease-out;
	-moz-transition: all 0.2s ease-out;
	-ms-transition: all 0.2s ease-out;
	-o-transition: all 0.2s ease-out;
	transition: all 0.2s ease-out
}

.b-icon-search__inner:after {/* 검색 아이콘 돋보기 색깔  */
	background-color: #06bfbf;
	content: " ";
	display: block;
	height: 3px;
	position: absolute;
	width: 10px;
	top: 18px;
	left: -4px;
	transform: rotate(-60deg);
	-moz-transform: rotate(-60deg);
	-webkit-transform: rotate(-60deg);
	-o-transform: rotate(-60deg);
	-ms-transform: rotate(-60deg);
	border-radius: 3px;
	-webkit-border-radius: 3px;
	-moz-border-radius: 3px
}

.b-icon-search--theme_secondary .b-icon-search__inner {
	border: 2px solid #fff
}

.`--theme_secondary .b-icon-search__inner:before {
	border: 1px solid #fff;
	border-color: transparent #fff transparent transparent
}

.b-icon-search--theme_secondary .b-icon-search__inner:after {
	background-color: #fff
}

.b-icon-search:hover .b-icon-search__inner:before {
	transform: rotate(360deg);
	-moz-transform: rotate(360deg);
	-webkit-transform: rotate(360deg);
	-o-transform: rotate(360deg);
	-ms-transform: rotate(360deg)
}

.b-nav-global .b-icon-search {
	float: left;
	padding: 18px 1em 22px
}

.b-icon-dm {
	border: 2px solid #fa6462;
	width: 22px;
	height: 15px;
	top: 1px;
	position: relative;
	border-radius: 3px;
	-webkit-border-radius: 3px;
	-moz-border-radius: 3px
}

.b-icon-dm__inner {
	position: absolute;
	left: 50%;
	margin-left: -1px
}

.b-icon-dm__inner:after, .b-icon-dm__inner:before {
	position: absolute;
	width: 1px;
	height: 8px;
	content: "";
	background-color: #fa6462
}

.b-icon-dm__inner:before {
	left: -8px;
	transform: rotate(-30deg);
	-moz-transform: rotate(-30deg);
	-webkit-transform: rotate(-30deg);
	-o-transform: rotate(-30deg);
	-ms-transform: rotate(-30deg)
}

.b-icon-dm__inner:after {
	left: 9px;
	transform: rotate(30deg);
	-moz-transform: rotate(30deg);
	-webkit-transform: rotate(30deg);
	-o-transform: rotate(30deg);
	-ms-transform: rotate(30deg)
}

.b-icon-dm__content {
	width: 14px;
	height: 0;
	top: 3px;
	border-top: 6px solid #fa6462;
	border-left: 3px solid white;
	border-right: 3px solid white;
	display: block;
	position: absolute;
	left: 50%;
	margin-left: -10px;
	background-color: #fa6462
}

.b-icon-dm:before, .b-icon-dm:after {
	content: "";
	position: absolute
}

.b-icon-dm:before {
	width: 16px;
	height: 6px;
	left: 2px;
	top: 8px;
	border-radius: 14px 14px 3px 3px;
	border: 1px solid #fa6462;
	border-color: #fa6462 #fa6462 transparent;
	z-index: 1
}

.b-icon-dm:after {
	width: 20px;
	height: 12px;
	border: 1px solid #fa6462;
	border-color: #fa6462;
	border-radius: 3px 3px 14px 14px;
	border-top-width: 2px;
	background-color: #FFF;
	top: -2px;
	left: 0;
	z-index: 2;
	-webkit-transition: all 0.1s ease-out;
	-moz-transition: all 0.1s ease-out;
	-ms-transition: all 0.1s ease-out;
	-o-transition: all 0.1s ease-out;
	transition: all 0.1s ease-out
}

.b-icon-dm--theme_secondary {
	border: 2px solid #fff
}

.b-icon-dm--theme_secondary .b-icon-dm__inner:before,
	.b-icon-dm--theme_secondary .b-icon-dm__inner:after {
	height: 11px;
	background-color: #fff
}

.b-icon-dm--theme_secondary .b-icon-dm__content {
	border-top: 6px solid #fff;
	background-color: #fff;
	visibility: hidden
}

.b-icon-dm--theme_secondary:before {
	border: 1px solid #fff;
	border-color: #fff #fff transparent;
	height: 5px;
	top: 9px
}

.b-icon-dm--theme_secondary:after {
	border: 1px solid #fff;
	border-color: #fff;
	background-color: transparent;
	height: 10px;
	width: 20px
}

.b-icon-dm--theme_secondary.is-open:after, .b-icon-dm--theme_secondary:hover:after
	{
	background-color: rgba(255, 255, 255, 0.3) !important
}

.b-icon-dm.is-open, .b-icon-dm:hover {
	border-radius: 0 0 3px 3px;
	border-top-width: 1px;
	top: 2px
}

.b-icon-dm.is-open:after, .b-icon-dm:hover:after {
	border-radius: 14px 14px 0 0;
	height: 7px;
	width: 22px;
	top: -8px;
	left: -2px;
	border-width: 1px 2px;
	background-color: rgba(250, 100, 98, 0.15)
