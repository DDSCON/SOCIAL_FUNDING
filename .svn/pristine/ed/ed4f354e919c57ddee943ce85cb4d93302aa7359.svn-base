.tabs {
    position: relative;
	margin: 40px auto;
	width: auto;
}

.tabs input {
	
	position: absolute;
	z-index: 1000;
	width: 120px;
	height: 20px;
	left: 0px;
	top: 0px;
	opacity: 0; 
    -ms-filter:"progid:DXImageTransform.Microsoft.Alpha(Opacity=0)";
    filter: alpha(opacity=0);
	cursor: pointer;
}
