.row, .notification-message, .site-nav, .user-options,
	.livesearch-header {
	*zoom: 1
}

.row:before, .notification-message:before, .site-nav:before,
	.user-options:before, .livesearch-header:before, .row:after,
	.notification-message:after, .site-nav:after, .user-options:after,
	.livesearch-header:after {
	content: " ";
	display: table
}

.row:after, .notification-message:after, .site-nav:after, .user-options:after,
	.livesearch-header:after {
	clear: both
}

.media .media-image, .primary-menu, .user-menu .dropdown-column,
