tabs_one {
  display: block;
  margin: 0 auto 20px;
  position: relative; }
  nav{width:520px;}
  
  tabs_one > nav {
    -webkit-user-select: none;
    -moz-user-select: none;
    user-select: none;
    font-size: 0;
    border-bottom: 1px solid #06bfbf; }
    tabs_one > nav > tab {
    
      border-radius: 10px 10px 10px 10px;
      cursor: pointer;
      display: inline-block;
      padding: 0 20px;
      line-height: 2em;
      font-size: 16px;
      color: #000;
      background: #FFF; }
       tabs_one > nav > tab:hover {
        color: #06bfbf; } 
       tabs_one > nav > tab.active, tabs_one > nav > tab[active='true'] {
        color: #06bfbf; } 
      tabs_one > nav > tab > span {
        display: block;
        pointer-events: none; }
  tabs_one > tab {
    display: block;
    padding: 20px 20px 0;
    background: white; }
    tabs_one > tab:before, tabs_one > tab:after {
      content: " ";
      display: table; }
    tabs_one > tab:after {
      clear: both; }
  tabs_one[alive='true'] > tab {
    display: none; }
    tabs_one[alive='true'] > tab.active, tabs_one[alive='true'] > tab[active='true'] {
      display: block; }

      .heading{
      font-family: "RixSGo L", "Apple SD Gothic Neo", "MalgunGothic", "Dotum";
	font-weight:bold;
      }
      