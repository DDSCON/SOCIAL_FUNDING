/* footer */
.b-keep_in_touch__item__link--facebook,
	.b-keep_in_touch__item__link--twitter {
	background: transparent url() 0 0 no-repeat;
	display: inline-block;
	width: 36px;
	height: 36px;
	background-image:
		url(https://d1pqmsyqo4bi9.cloudfront.net/assets/icons/foot_channels-03c71ded3d047af37412add93a7ba2ca.png);
	text-indent: -99999px
}
.b-keep_in_touch__item {
	
	margin-left: 10px;
	list-style-type: none
}
.b-keep_in_touch {
	margin-top: 0
}
.b-keep_in_touch__item__link--facebook:hover {
	background-position: 0 -72px
}
.b-keep_in_touch__item__link--twitter {
	background-position: 0 -144px
}
.b-keep_in_touch__item__link--twitter:hover {
	background-position: 0 -216px
}
.b-keep_in_touch__item__link--facebook {
	background: transparent url() 0 0 no-repeat;
	display: inline-block;
	width: 36px;
	height: 36px;
	background-image:
		url(https://d1pqmsyqo4bi9.cloudfront.net/assets/icons/foot_channels-03c71ded3d047af37412add93a7ba2ca.png);
	text-indent: -99999px
}
@media only screen and (-webkit-min-device-pixel-ratio: 2) , only screen and
		(min--moz-device-pixel-ratio: 2) , only screen and
		(-o-min-device-pixel-ratio: 2 / 1) , only screen and
		(min-device-pixel-ratio: 2) , only screen and (min-resolution: 192dpi)
		, only screen and (min-resolution: 2dppx) {
	
		.b-keep_in_touch__item__link--twitter {
		background-image:
			url(https://d1pqmsyqo4bi9.cloudfront.net/assets/icons/foot_channels@2x-ec97e91bc50707695b655eebe61ed4f2.png);
		-webkit-background-size: 36px auto;
		-moz-background-size: 36px auto;
		-o-background-size: 36px auto;
		background-size: 36px auto
	}
}
