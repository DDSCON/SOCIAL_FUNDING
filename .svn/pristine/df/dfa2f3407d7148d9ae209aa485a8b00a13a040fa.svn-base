.form-field{
	border-radius: 0;
	border-width: 1px;
	display: inline-block;
	float: left;
	height: 100%;
	
	vertical-align: middle;
	box-sizing: border-box
}

.form-field{
	font-size: 22px
}




.form-field {
	border-style: solid;
	border-width: 0;
	color: #7d7d7d;
	font-size: 1em;
	padding: 7.5px 5px;
    width: 625px;
	-moz-box-sizing: border-box;
	-webkit-box-sizing: border-box;
	box-sizing: border-box;
	border-radius: 3px;
}

.form-field-helper {
	border-style: solid;
	border-width: 1px;
	padding: 7.5px 5px;
	color: #adadad;
	font-weight: 600
}

.form-field:focus {
	background-color: #FFF;
	border-color: #fff #ddd #ddd #fff;
	box-shadow: 0 0 0px 3px #06bfbf;
	position: relative;
	z-index: 99;
	transition: background-color 200ms ease-out, border-color 200ms ease-out
}

.ui-form-group .form-field:focus ~ .b-button {
	box-shadow: 0 0 0px 3px #fa6462;
	border-color: #fa6462
}

.ui-form-group .form-field:focus ~ .b-button--theme_tertiary {
	box-shadow: 0 0 0px 3px #81b0b0;
	border-color: #81b0b0
}

.form-field.is-disabled {
	color: #bfb0b0;
	cursor: not-allowed
}

.form-field--size_s {
	padding-top: 5px;
	padding-bottom: 5px
}

.form-field--theme_clean {
	background-color: #FFF;
	border-color: #fff #ddd #ddd #fff
}

.form-field--theme_bare {
	background-color: rgba(0, 0, 0, 0.3);
	border-color: transparent;
	color: #c8c8c8
}

.form-field--theme_bare:focus {
	background-color: rgba(0, 0, 0, 0.3);
	border-color: transparent;
	box-shadow: none
}

.form-field--type_search {
